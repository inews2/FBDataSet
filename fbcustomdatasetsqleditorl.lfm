  object quGetObjLIst: TUIBQuery
    SQL.Strings = (
      'select'
      '  0 as obj_flag,'
      '  rdb$relations.rdb$relation_name as relation_name'
      'from'
      '  rdb$relations'
      'where'
      '    rdb$relations.rdb$system_flag = 0'
      '  and'
      '    rdb$relations.rdb$view_blr is null'
      ''
      'union all'
      ''
      'select'
      '  1,'
      '  rdb$relations.rdb$relation_name as relation_name'
      'from'
      '  rdb$relations'
      'where'
      '    rdb$relations.rdb$system_flag = 0'
      '  and'
      '    rdb$relations.rdb$view_blr is not null'
      ''
      'union all'
      ''
      'select'
      '  2,'
      '  rdb$procedures.rdb$procedure_name'
      'from'
      '  rdb$procedures'
      'where'
      '  rdb$procedures.rdb$system_flag = 0'
      ''
      'order by'
      '  1, 2'
    )
    CachedFetch = False
    Left = 208
    Top = 80
  end
