  object quFieldsList: TUIBQuery
    SQL.Strings = (
      'select'
      '  rdb$relation_fields.RDB$FIELD_NAME,'
      '  rdb$relation_fields.rdb$description,'
      '  (case'
      '   when rdb$fields.rdb$computed_blr is null then 0'
      '   else 1'
      '   end) as calc_field_flag'
      ''
      'from'
      '  rdb$relation_fields'
      '  inner join rdb$fields on (rdb$fields.rdb$field_name = rdb$relation_fields.rdb$field_source)'
      'where'
      '  rdb$relation_fields.rdb$relation_name = :relation_name'
      'order by'
      '  rdb$relation_fields.rdb$field_position'
    )
    Left = 277
    Top = 80
  end
